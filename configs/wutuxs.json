{
  "site": "https://www.wutuxs.com",
  "charset": "gbk",
  "search": {
    "method": "post",
    "pattern": "https://www.wutuxs.com/modules/article/search.php?searchkey=${key}",
    "bookList": "id.content@tag.tbody@tag.tr",
    "bookName": "tag.td.0@tag.a@text",
    "bookUrl": "tag.td.0@tag.a@href",
    "latestChapter": "tag.td.1@tag.a@text",
    "author": "tag.td.2@text",
    "isSingle": "tag.caption",
    "singleBookName": "tag.h1@text## 全文阅读",
    "singleAuthor": "id.at@tag.td.1@text",
    "singleBookUrl": "class.read@href",
    "singleLatestChapter": "tag.meta[name='description']@content##.*最新章节 (.*?) 由.*$_$$1"
  },
  "list": {
    "chapters": "id.at@tag.td@tag.a",
    "title": "text",
    "href": "href"
  },
  "detail": {
    "description": "class.book@class.js@tag.p.0@text",
    "imageUrl": "class.info@class.pic@tag.img@src",
    "latest": "class.news@class.fl@tag.a.0@text",
    "author": "class.btitle@tag.i.1@text",
    "name": "class.btitle@tag.h1@text"
  },
  "chapter": {
    "title": "tag.h1@text",
    "content": "id.contents@html"
  }
}
